        <!-- Scripts para el mapa -->
        <script src="https://api.mapbox.com/mapbox-gl-js/v3.2.0/mapbox-gl.js"></script>
        <link href="https://api.mapbox.com/mapbox-gl-js/v3.2.0/mapbox-gl.css" rel="stylesheet">
        <script>
            mapboxgl.accessToken = 'pk.eyJ1IjoiYWxleHhvbWJ5IiwiYSI6ImNsaWo4c3d6NzA1NnMzZXBiZjE2a2I0aDAifQ.nVog7tI8-GkimJfWeb-hIg';
            var map = new mapboxgl.Map({
                container: 'map',
                style: 'mapbox://styles/mapbox/streets-v11',
                center: [-99.17491900, 19.45388900], // Nuevas coordenadas del CECYT 9
                zoom: 17 // Ajusta el nivel de zoom según sea necesario
            });
        
            var marker = new mapboxgl.Marker()
                .setLngLat([-99.17491901, 19.45388901]) // Nuevas coordenadas del marcador
                .addTo(map);
        </script>
